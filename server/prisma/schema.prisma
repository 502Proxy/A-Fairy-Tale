// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" 
  url      = env("DATABASE_URL")
}

model Resident {
  id    String  @id @default(cuid()) 
  name  String
  role  String
  bio   String?
  image String? 

  createdAt DateTime @default(now()) 
  updatedAt DateTime @updatedAt      
}